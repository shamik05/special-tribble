{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-props-no-spreading": "warn",
        "no-nested-ternary": "warn",
        "no-unused-vars": "warn",
        "react/prop-types": "warn",
        "import/no-extraneous-dependencies": ["warn"],
        "react/jsx-no-bind": "warn"
    }
}
